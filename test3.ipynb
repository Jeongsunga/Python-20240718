    "def solution(array, queries):\n",
    "    for i in range(len(queries)):\n",
    "        for j in range(queries[i][0], queries[i][1] + 1):\n",
    "            array[j] += 1\n",
    "    return array\n",
    "\n",
    "array = [0, 1, 2, 3, 4]\n",
    "queries = [[0, 1],[1, 2],[2, 3]]\n",
    "solution(array, queries)\n",
    "print(array)\n"
   ]
  }
